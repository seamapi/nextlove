{
  "name": "nextlove",
  "version": "2.4.0",
  "private": false,
  "repository": "https://github.com/seamapi/nextlove",
  "files": [
    "dist",
    "generators.d.ts",
    "exception-middleware.d.ts"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "bin": {
    "nsm": "nsm-bin.js",
    "nextlove": "bin.js"
  },
  "main": "./dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./generators": {
      "types": "./dist/generators/index.d.ts",
      "import": "./dist/generators/index.mjs",
      "require": "./dist/generators/index.js"
    },
    "./exception-middleware": {
      "types": "./dist/nextjs-exception-middleware/index.d.ts",
      "import": "./dist/nextjs-exception-middleware/index.mjs",
      "require": "./dist/nextjs-exception-middleware/index.js"
    }
  },
  "scripts": {
    "test": "npm run typecheck && npm run ava",
    "typecheck": "tsc --noEmit",
    "ava": "ava",
    "build": "tsup --dts --sourcemap inline src/index.ts src/generators/index.ts src/nextjs-exception-middleware/index.ts --format cjs,esm",
    "yalc": "npm run build && yalc push"
  },
  "peerDependencies": {
    "esbuild-register": ">=3",
    "next": ">=12",
    "nextjs-server-modules": ">=1",
    "prettier": "^2.0.0 || ^3.0.0",
    "react": ">=18",
    "react-dom": ">=18",
    "zod": "^3.0.0"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "openapi3-ts": "^4.1.2",
    "zod-to-ts": "^1.1.4"
  },
  "devDependencies": {
    "@anatine/zod-openapi": "^2.0.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "18.0.0",
    "@types/prettier": "^2.7.1",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@types/sinon": "10.0.8",
    "ava": "^5.3.1",
    "chalk": "^5.3.0",
    "esbuild": "^0.14.7",
    "esbuild-register": "^3.3.3",
    "esbuild-runner": "^2.2.1",
    "eslint": "8.18.0",
    "eslint-config-next": "12.2.0",
    "expect-type": "^0.15.0",
    "globby": "^13.2.2",
    "next": "12.2.0",
    "nextjs-server-modules": "^2.1.0",
    "prettier": "^2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sinon": "^15.2.0",
    "ts-toolbelt": "^9.6.0",
    "tsup": "^5.6.3",
    "turbo": "^1.3.1",
    "type-fest": "^3.1.0",
    "typescript": "^5.0.2",
    "zod": "^3.21.4"
  }
}
